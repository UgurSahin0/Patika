import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Sayı giriniz: ");
        int sayi = input.nextInt();
        int faktoriyel = 1;
        for (int i = 1;i<=sayi;i++){
            faktoriyel*=i;
        }
        System.out.print("Sonuç: "+faktoriyel);
    }
}
